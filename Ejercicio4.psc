	Escribir "Ingresa el segundo numero: "
	Leer num2
	suma = num1+ num2
	resta = num1-num2
	division = num1/num2
	multiplicacion = num1*num2
	Escribir "Suma= ", suma
	Escribir "Resta= ", resta
	Escribir "División= ", division
	Escribir "Multiplicación= ", multiplicacion
FinAlgoritmo
